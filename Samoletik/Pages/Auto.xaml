<Page x:Class="Samoletik.Pages.Auto"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Samoletik.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Auto">

    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Top" Height="345" Width="224" Margin="0,109,0,0">
            <TextBlock Text="Логин:"/>
            <TextBox x:Name="txtbLogin"/>
            <TextBlock Text="Пароль:"/>
            <PasswordBox x:Name="pswbPassword"/>


            <StackPanel Orientation="Horizontal" Height="80" HorizontalAlignment="Center">
                <Button x:Name="btnEnter" Margin="5,15,5,5" Content="Войти" Click="btnEnter_Click" Width="131" Height="36" Background="#FF52D6E6"/>

            </StackPanel>
        </StackPanel>
            </Grid>
</Page>
